{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "func-names": ["error", "never"],
      "linebreak-style": 0,
      "react/destructuring-assignment": 0,
      "react/jsx-indent": ["error", 4],
      "react/jsx-indent-props": ["error", 4],
      "jsx-a11y/anchor-is-valid": 0,
      "react/prefer-stateless-function" :0,
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"],
          "extensions": [".js", ".jsx", ".svg"]
        }
      }
    }
}